{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/",
      "dest": "/my-animal-rescue/pages/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/my-animal-rescue/$1"
    }
  ]
} 